{
  "name": "bundler-portal",
  "version": "0.0.0",
  "description": "Webcomponent bundler-portal following open-wc recommendations",
  "author": "bundler-portal",
  "license": "MIT",
  "scripts": {
    "start": "es-dev-server --app-index index.html --node-resolve --watch",
    "start:build": "es-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "graphql-tag": "^2.10.3",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "mobx": "^5.15.4",
    "@adobe/lit-mobx": "^0.0.3",
    "@vaadin/vaadin-button": "^2.2.2",
    "@vaadin/vaadin-grid": "^5.5.3",
    "@vaadin/vaadin-text-field": "^2.5.5",
    "@vaadin/vaadin-notification": "^1.4.0"
  },
  "devDependencies": {
    "es-dev-server": "^1.5.0",
    "@open-wc/building-rollup": "^0.21.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.4"
  }
}